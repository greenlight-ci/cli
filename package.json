  },
  "devDependencies": {
    "strip-ansi": "^5.0.0",
    "tap": "^12.0.1"
  },
  "dependencies": {
    "@greenlight/config-loader": "^2.2.0",
    "@greenlight/docker": "^1.6.1",
    "chalk": "^2.4.0",
    "logress": "^2.0.2",
    "make-dir": "^2.0.0",
    "smart-promise": "^2.0.0",
    "text-table": "^0.2.0",
    "yargs": "^12.0.2"
  }
}
